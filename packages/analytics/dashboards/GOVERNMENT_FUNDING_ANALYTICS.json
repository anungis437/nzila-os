  "filters": {
    "program": {
      "type": "multi-select",
      "options": ["SR&ED", "IRAP", "SDTC", "MITACS", "Strategic Innovation Fund", "All"],
      "default": "All"
    },
    "status": {
      "type": "multi-select",
      "options": ["Approved", "Pending", "Planned", "Projected", "All"],
      "default": "All"
    },
    "year": {
      "type": "single-select",
      "options": [2026, 2027, 2028],
