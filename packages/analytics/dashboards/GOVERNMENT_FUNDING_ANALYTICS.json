  "alerts": [
    {
      "alert_id": "deadline_approaching",
      "condition": "event.date <= today + 30 days AND event.status = 'upcoming'",
      "severity": "warning",
      "message": "Upcoming deadline: {event.event} on {event.date}"
    },
    {
      "alert_id": "application_overdue",
      "condition": "event.date < today AND event.status = 'upcoming' AND event.type = 'application'",
      "severity": "critical",
      "message": "Overdue application: {event.event}"
    },
    {
      "alert_id": "funding_gap",
      "condition": "total_funding_2026 < 800000",
      "severity": "warning",
      "message": "2026 funding below target ($800K). Accelerate IRAP applications."
    }
  ],
  
