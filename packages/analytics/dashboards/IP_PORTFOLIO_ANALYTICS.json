      "section_id": "ip_portfolio_overview",
      "name": "IP Portfolio Overview",
      "type": "kpi_grid",
      "layout": "2x3",
      "widgets": [
        {
          "widget_id": "total_portfolio_value",
          "name": "Total IP Portfolio Valuation",
          "type": "kpi",
          "value": 5500000,
          "value_range": {
            "low": 5000000,
            "high": 6000000
          },
          "unit": "currency",
          "target": 8000000,
          "status": "on-track",
          "breakdown": {
            "source_code": 4000000,
            "algorithms_ai_models": 800000,
            "trademarks_brand": 250000,
            "trade_secrets": 450000
          }
        },
        {
          "widget_id": "patents_filed",
          "name": "Patents Filed",
          "type": "kpi",
          "value": 3,
          "target": 5,
          "status": "on-track",
          "breakdown": {
            "pending": 3,
            "granted": 0,
            "rejected": 0
          }
        },
        {
          "widget_id": "trademarks_registered",
          "name": "Trademarks",
          "type": "kpi",
          "value": 7,
          "target": 10,
          "status": "on-track",
          "breakdown": {
            "registered": 1,
            "pending": 4,
            "to_file": 2,
            "planned": 3
          }
        },
        {
          "widget_id": "trade_secrets_protected",
          "name": "Trade Secrets Protected",
          "type": "kpi",
          "value": 7,
          "target": 10,
          "status": "healthy",
          "breakdown": {
            "high_protection": 4,
            "medium_protection": 2,
            "action_required": 1
          }
        },
        {
          "widget_id": "licensing_revenue_potential",
          "name": "Licensing Revenue Potential",
          "type": "kpi",
          "value": 450000,
          "unit": "currency",
          "target": 600000,
          "status": "approaching",
          "calculation": "Annual licensing revenue if all patents granted + trade secrets licensed"
        },
        {
          "widget_id": "ip_protection_status",
          "name": "IP Protection Status",
          "type": "kpi",
          "value": 75,
          "unit": "percent",
          "target": 90,
          "status": "approaching",
          "calculation": "Protected IP / Total IP assets"
        }
      ]
    },
    
