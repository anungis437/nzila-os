      "options": ["Registered", "Pending", "Planned", "All"],
      "default": "All"
    }
  },
  
  "alerts": [
    {
      "alert_id": "patent_filing_deadline",
      "condition": "filing_date <= today + 30 days AND status = 'Planned'",
      "severity": "warning",
      "message": "Patent filing deadline approaching: {patent_title} on {filing_date}"
    },
    {
      "alert_id": "trademark_renewal",
      "condition": "renewal_date <= today + 180 days AND status = 'Registered'",
      "severity": "warning",
      "message": "Trademark renewal due: {trademark} on {renewal_date}"
    },
    {
      "alert_id": "trade_secret_audit_overdue",
      "condition": "next_audit < today AND protection_level = 'HIGH'",
      "severity": "high",
      "message": "Trade secret audit overdue: {trade_secret}"
    },
    {
      "alert_id": "ip_protection_low",
      "condition": "ip_protection_status < 80",
      "severity": "warning",
      "message": "IP protection status below 80%. Review unprotected IP assets."
    }
  ],
  
  "dependencies": [
    "business/intellectual-property/ip-protection-strategy.md",
    "business/intellectual-property/patent-portfolio.md",
    "business/intellectual-property/trade-secrets-inventory.md",
    "corporate/legal/nda-templates.md"
  ]
}
